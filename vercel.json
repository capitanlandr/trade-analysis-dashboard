{
  "version": 2,
  "builds": [
    {
      "src": "dashboard/frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "dashboard/backend/src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dashboard/backend/src/server.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/dashboard/frontend/dist/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}